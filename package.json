{
  "name": "crane",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "clean": "npm rm -rf build && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/app.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "lint": "eslint src/  --fix",
    "seeder": "babel-node ./src/seeder/seeder.js",
    "test": "cross-env NODE_ENV=test && npm run seeder && nyc --require --reporter=html --reporter=text mocha -r @babel/register ./src/tests/index.js --timeout 1000000 --recursive --exit || true",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov --reporter=lcov | node ./node_modules/coveralls/bin/coveralls.js --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donaldcrane/crane.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/donaldcrane/crane/issues"
  },
  "homepage": "https://github.com/donaldcrane/crane#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "@babel/runtime": "^7.13.17",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cookie-session": "^1.4.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.3",
    "mocha": "^8.3.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.5.0",
    "bcrypt": "^5.0.0",
    "cloudinary": "^1.25.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "mongoose": "^6.0.12",
    "mongoose-seed": "^0.6.0",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "path": "^0.12.7",
    "pino": "^6.2.1",
    "pino-pretty": "^7.0.0",
    "validator": "^13.6.0"
  }
}
